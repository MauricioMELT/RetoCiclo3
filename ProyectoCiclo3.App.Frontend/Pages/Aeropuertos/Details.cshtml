@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsAeropuertosModel
@{
    var aeropuerto = Model.Aeropuertos;
}
 
<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id: </b> @aeropuerto.id</li>
  <li class="list-group-item"> <b>Nombre: </b> @aeropuerto.nombre</li>
